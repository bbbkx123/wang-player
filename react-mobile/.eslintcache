onfig = {\r\n  immediate: boolean;\r\n};\r\n\r\nexport function useWatch<T>(dep: T, callback: Callback<T>, config: Config = { immediate: false }) {\r\n  const { immediate } = config;\r\n\r\n  const prev = useRef<T>();\r\n  const inited = useRef(false);\r\n  const stop = useRef(false);\r\n\r\n  useEffect(() => {\r\n    const execute = () => callback(prev.current);\r\n\r\n    if (!stop.current) {\r\n      if (!inited.current) {\r\n        inited.current = true;\r\n        if (immediate) {\r\n          execute();\r\n        }\r\n      } else {\r\n        execute();\r\n      }\r\n      prev.current = dep;\r\n    }\r\n  }, [dep]);\r\n\r\n  return () => {\r\n    stop.current = true;\r\n  };\r\n}\r\n","E:\\web-project\\wang-player1\\react-mobile\\src\\service\\http.ts",[],"E:\\web-project\\wang-player1\\react-mobile\\src\\service\\config.ts",["73"],"export default {\r\n  baseUrl: process.env.NODE_ENV === \"development\" ? \"http://localhost:3000\" : \"http://106.13.207.132:3000\"\r\n}","E:\\web-project\\wang-player1\\react-mobile\\src\\service\\index.tsx",[],"E:\\web-project\\wang-player1\\react-mobile\\src\\utils\\hook.ts",["74"],{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","severity":1,"message":"80","line":14,"column":21,"nodeType":"81","messageId":"82","endLine":14,"endColumn":33},{"ruleId":"79","severity":1,"message":"83","line":18,"column":10,"nodeType":"81","messageId":"82","endLine":18,"endColumn":16},{"ruleId":"79","severity":1,"message":"84","line":19,"column":16,"nodeType":"81","messageId":"82","endLine":19,"endColumn":23},{"ruleId":"79","severity":1,"message":"85","line":21,"column":10,"nodeType":"81","messageId":"82","endLine":21,"endColumn":18},{"ruleId":"79","severity":1,"message":"86","line":73,"column":44,"nodeType":"81","messageId":"82","endLine":73,"endColumn":50},{"ruleId":"87","severity":1,"message":"88","line":135,"column":6,"nodeType":"89","endLine":135,"endColumn":8,"suggestions":"90"},{"ruleId":"87","severity":